set backspace=2
set tabstop=4
set nu
set relativenumber
set mouse=a
set nocompatible
" vundle
filetype off
set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()
Plugin 'gmarik/Vundle.vim'
Plugin 'delimitMate.vim'
Plugin 'sheerun/vim-polyglot'
call vundle#end()
filetype plugin indent on
syntax on
imap jj <Esc>
